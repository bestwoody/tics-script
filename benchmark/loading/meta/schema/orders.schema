CREATE TABLE IF NOT EXISTS orders (
	O_ORDERKEY       UInt8,
	O_CUSTKEY        UInt8,
	O_ORDERSTATUS    FixedString(1),
	O_TOTALPRICE     Float64,
	O_ORDERDATE      Date,
	O_ORDERPRIORITY  String,
	O_CLERK          String,
	O_SHIPPRIORITY   UInt8,
	O_COMMENT        String
) ENGINE = MergeTree(O_ORDERDATE, (O_ORDERKEY, O_ORDERDATE), 8192);
