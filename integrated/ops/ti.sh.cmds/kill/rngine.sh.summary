#!/bin/bash

function cmd_ti_kill_rngine()
{
	local ti_file="${1}"
	local ti_args="${2}"
	local cmd_mod_names="${3}"
	local cmd_hosts="${4}"
	local cmd_indexes="${5}"
	local mods="${6}"

	local index=`from_mods_random_mod "${mods}" 'rngine'`
	if [ ! -z "${index}" ]; then
		"${integrated}/ops/ti.sh" -h "${cmd_hosts}" -m 'rngine' -i "${index}" -k "${ti_args}" "${ti_file}" 'fstop'
	fi
}

source "`cd $(dirname ${BASH_SOURCE[0]}) && pwd`/_env.sh"
auto_error_handle
cmd_ti_kill_rngine "${@}"
