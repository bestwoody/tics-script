#!/bin/bash

here=`cd $(dirname ${BASH_SOURCE[0]}) && pwd`
source "${here}/_env.sh"
auto_error_handle

ti_file="${1}"
ti_args="${2}"
cmd_mod_names="${3}"
cmd_hosts="${4}"
cmd_indexes="${5}"
mods="${6}"

index=`random_mod "${mods}" 'tikv'`
if [ ! -z "${index}" ]; then
	"${integrated}/ops/ti.sh" -m 'tikv' -i "${index}" -k "${ti_args}" "${ti_file}" 'fstop'
fi
