#!/bin/bash

source "`cd $(dirname ${BASH_SOURCE[0]}) && pwd`/_env.sh"
auto_error_handle

if [ -z "${1+x}" ]; then
	echo "usage: <func> force_clean" >&2
	exit 1
fi
force_clean="${1}"
if [ -z "${force_clean}" ]; then
	force_clean="false"
fi
assert_prev_test_finished "${BASH_SOURCE[0]}" "${force_clean}"

ti="${integrated}/ops/ti.sh"
test_entry_dir=`get_test_entry_dir "${BASH_SOURCE[0]}"`

ti_file="${test_entry_dir}/test.ti"

if [ -f "${ti_file}" ]; then
	"${ti}" "${ti_file}" burn doit
fi
